    "ext-json": "*",
    "ext-zip": "*",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
    "enlightn/security-checker": "^1.7.0",
    "ergebnis/composer-normalize": "^2.13",
    "katzgrau/klogger": "dev-master",
    "php-parallel-lint/php-console-highlighter": "0.*",
